doctype 5
html(lang="en")
  head
    <script src="assets/meny.min.js"></script>
    <script src="assets/menySetup.js"></script>
    link(rel='stylesheet', href='assets/blog.css')
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,700' rel='stylesheet' type='text/css'>
    

  body
    div.meny
      ul
        for page in index
          li
            - title = page.replace('.html', '');
            a(href='#{page}') #{title}
    div.meny-arrow
    div.contents
      include ../posts/#{postfile}

